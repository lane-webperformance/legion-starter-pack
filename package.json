{
  "name": "legion-starter-pack",
  "version": "0.0.3",
  "description": "Run some example testcases, or use this as a seed to start writing your own.",
  "scripts": {
    "start": "eslint testcase.js && node testcase.js",
    "test": "eslint main.js ./examples/** && npm run 001_simple && npm run 002_ticket",
    "001_simple": "node ./examples/001_simple.js",
    "002_ticket": "node ./examples/002_ticket.js",
    "clean": "rimraf node_modules"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "command-line-args": "^3.0.0",
    "command-line-usage": "^3.0.1",
    "legion": "git+https://github.com/lane-webperformance/legion.git#v0.0.13",
    "legion-io-fetch": "git+https://github.com/lane-webperformance/legion-io-fetch.git#v0.0.7",
    "legion-obstacle-course": "git+https://github.com/lane-webperformance/legion-obstacle-course.git#v0.0.6"
  },
  "devDependencies": {
    "eslint": "^2.11.1",
    "rimraf": "^2.5.2"
  }
}
